<h1>アーティスト</h1>

<%= form_for(@artist) do |f| %>

<h4>アーティスト名</h4>
<%= f.text_field :artist_name %>

<%= f.attachment_field :artist_image %>

<%= f.submit '登録' %>

<% end %>

<% @artists.each do |artist| %>
<%= artist.artist_name %>
<%= attachment_image_tag artist, :artist_image, size: "100x100" %>
<% end %>